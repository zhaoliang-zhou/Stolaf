JKKo","colab_type":"code","colab":{}},"cell_type":"code","source":["numSteps = 100000\n","currState = random.randrange(N)  # choose a random starting state\n","statesVisited1 = np.zeros(numSteps)\n","sig2 = 1"],"execution_count":0,"outputs":[]},{"metadata":{"id":"3XiLx9OhJfP3","colab_type":"code","colab":{}},"cell_type":"code","source":[""],"execution_count":0,"outputs":[]},{"metadata":{"id":"CXjsS8pSJlrW","colab_type":"code","outputId":"e4122d22-1965-4b81-d88b-494129750ef7","executionInfo":{"status":"ok","timestamp":1556559547473,"user_tz":300,"elapsed":1163,"user":{"displayName":"Zhaoliang Zhou","photoUrl":"","userId":"11420802199847110781"}},"colab":{"base_uri":"https://localhost:8080/","height":283}},"cell_type":"code","source":["for i in range(numSteps):\n","  statesVisited1[i] = currState\n","  currState = doMove1(currState, sig2)\n","plt.hist(statesVisited1, bins=30, alpha=0.8, linewidth=1, edgecolor='#202020')\n","plt.xlabel(\"state index\")\n","plt.ylabel(\"frequency\");"],"execution_count":0,"outputs":[{"output_type":"display_data","data":{"image/png":"iVBORw0KGgoAAAANSUhEUgAAAY4AAAEKCAYAAAAFJbKyAAAABHNCSVQICAgIfAhkiAAAAAlwSFlz\nAAALEgAACxIB0t1+/AAAADl0RVh0U29mdHdhcmUAbWF0cGxvdGxpYiB2ZXJzaW9uIDMuMC4zLCBo\ndHRwOi8vbWF0cGxvdGxpYi5vcmcvnQurowAAF0JJREFUeJzt3X+0XWV95/H3R1D8OUAgRgxg4hK1\n6FTFFHEydapxEB1HslpELNXoYmSmC8Ef7epoZYbR4qxhtVN/zGqRKGh0GBEZBxl1gTSiVatoUJRf\nUjKAQiTJjSB2ySoV8p0/9nPhGu/NPTvec+85ue/XWmfdvZ/9nHO+OTnJ5+5n7/3sVBWSJA3qEQtd\ngCRpvBgckqReDA5JUi8GhySpF4NDktSLwSFJ6sXgkCT1YnBIknoxOCRJvey70AUMw8EHH1wrVqxY\n6DIkaaxcc801O6pq6Wz99srgWLFiBZs2bVroMiRprCT54SD9HKqSJPVicEiSejE4JEm9GBySpF4M\nDklSLwaHJKkXg0OS1IvBIUnqxeCQJPWyV145ruE58eR1bNm6bdZ+y5+0jIsv3DDvrydp+AwO9bJl\n6zaWrD1z9n6Xnr0grydp+ByqkiT1YnBIknoxOCRJvRgckqReDA5JUi8GhySpF4NDktSLwSFJ6sXg\nkCT1YnBIknpxyhENxcTEdlavOW7WfjsmdrBkHuqRNHcMDg3Fzp010BxU2847fR6qkTSXDA6NhUH3\nYJxFVxo+g0NjYdA9GGfRlYbPg+OSpF6GGhxJ3pbkhiTXJ/lkkkcnWZnk6iSbk3wqyaNa3/3a+ua2\nfcWU13lna785ycuGWbMkafeGFhxJlgNnAKuq6tnAPsBJwDnA+6rqacA9wCntKac